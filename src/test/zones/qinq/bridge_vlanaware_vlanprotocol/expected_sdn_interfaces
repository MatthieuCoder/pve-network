#version:1

auto ln_myzone
iface ln_myzone
	link-type veth
	veth-peer-name pr_myzone

auto myvnet
iface myvnet
	bridge_ports z_myzone.100
	bridge_stp off
	bridge_fd 0

auto pr_myzone
iface pr_myzone
	link-type veth
	veth-peer-name ln_myzone

auto vmbr0
iface vmbr0
	bridge-vlan-protocol 802.1ad

auto z_myzone
iface z_myzone
	bridge-stp off
	bridge-ports vmbr0.10 ln_myzone
	bridge-fd 0
	bridge-vlan-aware yes
	bridge-vids 2-4094
