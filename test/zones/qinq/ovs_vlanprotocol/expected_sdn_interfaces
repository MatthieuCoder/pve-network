#version:1

auto myvnet
iface myvnet
	bridge_ports z_myzone.100
	bridge_stp off
	bridge_fd 0

auto sv_myzone
iface sv_myzone
	ovs_type OVSIntPort
	ovs_bridge vmbr0
	ovs_options vlan_mode=dot1q-tunnel tag=10 other_config:qinq-ethtype=802.1ad

auto vmbr0
iface vmbr0
	ovs_ports sv_myzone

auto z_myzone
iface z_myzone
	bridge-stp off
	bridge-ports sv_myzone
	bridge-fd 0
	bridge-vlan-aware yes
	bridge-vids 2-4094
